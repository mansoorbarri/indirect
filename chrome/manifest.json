{
  "manifest_version": 2,
  "name": "Instagram Redirector",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "*://www.instagram.com/*",
    "*://instagram.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "logo/icon16.png",
    "32": "logo/icon32.png",
    "48": "logo/icon48.png",
    "128": "logo/icon128.png"
  }
}
